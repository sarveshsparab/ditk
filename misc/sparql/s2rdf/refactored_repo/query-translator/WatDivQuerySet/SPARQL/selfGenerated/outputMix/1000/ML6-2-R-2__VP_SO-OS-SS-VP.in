#mapping v0 wsdbm:Retailer uniform
SELECT ?v1 ?v2 ?v3 ?v4 ?v5 ?v6 WHERE {
	wsdbm:Retailer4777	gr:offers	?v1 .
	?v1	gr:includes	?v2 .
	?v2	rev:hasReview	?v3 .
	?v3	rev:reviewer	?v4 .
	?v4 wsdbm:likes ?v5 .
	?v5	sorg:description	?v6 .
}